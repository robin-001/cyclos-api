{
    "name": "angstrom/cyclos-api",
    "description": "Laravel package for interacting with Cyclos API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Angstrom",
            "email": "info@angstrom.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Angstrom\\CyclosApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angstrom\\CyclosApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Angstrom\\CyclosApi\\CyclosApiServiceProvider"
            ],
            "aliases": {
                "CyclosApi": "Angstrom\\CyclosApi\\Facades\\CyclosApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
